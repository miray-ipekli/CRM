
@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="tr">

<!-- Head Partial -->
@await Html.PartialAsync("~/Views/UILayoutPartial/_HeadPartial.cshtml")


<body class="g-sidenav-show  bg-gray-100">

    <!-- Navbar Partial -->
    @await Html.PartialAsync("~/Views/UILayoutPartial/_NavbarPartial.cshtml")
    
    <main class="main-content position-relative max-height-vh-100 h-100 mt-1 border-radius-lg ">

        <!-- Navbar2 Partial -->
        @await Html.PartialAsync("~/Views/UILayoutPartial/_Navbar2Partial.cshtml")

        <div class="container-fluid py-4">
            @RenderBody()

            <!-- Footer Partial -->
            @await Html.PartialAsync("~/Views/UILayoutPartial/_FooterPartial.cshtml")
        </div>
    </main>
   
    <!-- Scripts Partial -->
    @await Html.PartialAsync("~/Views/UILayoutPartial/_ScriptsPartial.cshtml")
</body>

</html>